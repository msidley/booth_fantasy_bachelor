<div class="page-header">
  <h3>Cast #<%= @cast.id %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>Name</dt>
      <dd><%= @cast.name %></dd>

      <dt>Bio</dt>
      <dd><%= @cast.bio %></dd>

      <dt>Cast img</dt>
      <dd><%= @cast.cast_img %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/casts" class="btn btn-primary">
        Back
      </a>
      <a href="/casts/<%= @cast.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_cast/<%= @cast.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
